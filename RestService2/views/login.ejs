<%- include('components/header') %>
<div class="row">
    <div class="alert alert-danger" id='warningDiv'>Warning: Not using SSL/TLS</div>
      <form onsubmit='return false;'>
        <div class="form-group">
            <label>Email Address</label>
            <input type="email" name="email" class="form-control" placeholder="Email Address" id="emailLogin" />
        </div><!-- form-group -->

        <div class="form-group">
            <label>Password</label>
            <input type="password" name='password' class="form-control" placeholder="Password" id="passwordLogin" />
        </div><!-- form-group -->
        <button type="submit" onclick='login();' class="btn btn-primary">Login</button>
      </form>
</div><!-- row -->
<%- include('components/footer') %>
