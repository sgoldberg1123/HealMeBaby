<%- include('components/header') %>
<div class="row">
    <div class="alert alert-danger" id='warningDiv'>Warning: Not using SSL/TLS</div>
      <form onsubmit='return false;'>
        <div class="form-group">
            <label>Email Address</label>
            <input type="email" name='email' class="form-control" placeholder="EmailAddress" id="emailInsert" />
        </div><!-- form-group -->

        <div class="form-group">
            <label>First Name</label>
            <input type="firstName" name="firstName" class="form-control" placeholder="First Name" id="firstNameInsert" />
        </div> <!--form-group -->

        <div class="form-group">
            <label>Last Name</label>
            <input type="lastName" name="lastName" class="form-control" placeholder="Last Name" id="lastNameInsert" />
        </div>

        <div class="form-group">
            <label>Password</label>
            <input type="password" name='password' class="form-control" placeholder="Password" id="passwordInsert" />
        </div><!-- form-group -->

        <button type="submit" onclick="insert();" class="btn btn-primary">Submit</button>
      </form>
</div><!-- row -->
<%- include('components/footer') %>
